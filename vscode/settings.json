{
  "explorer.compactFolders": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "breadcrumbs.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": true,
  "editor.wordWrap": "off",
  "editor.fontFamily": "\"Berkeley Mono\", \"DankMono Nerd Font\", \"Berkeley Mono\", Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15.6,
  "editor.fontWeight": "500",
  "editor.suggest.showKeywords": false,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.lightbulb.enabled": "off",
  "editor.folding": true,
  "editor.showFoldingControls": "never",
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 0,
  "editor.renderWhitespace": "none",
  "editor.rulers": [100, 120],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollBeyondLastLine": true,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.insertMode": "replace",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off"
  },
  "editor.hover.delay": 300,
  "editor.quickSuggestionsDelay": 200,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": false,
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "errorLens.enabled": true,
  "eslint.validate": ["javascript", "typescript", "typescriptreact"],
  "explorer.confirmDelete": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "githubPullRequests.pullBranch": "never",
  "javascript.suggest.paths": false,
  "material-icon-theme.folders.color": "#7862d0",
  "prettier.requireConfig": false,
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true,
    "**/dist": true
  },
  "search.useGlobalIgnoreFiles": true,
  "telemetry.telemetryLevel": "off",
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "DankMono Nerd Font",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.scrollback": 1000000,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enablePersistentSessions": true,
  "todo-tree.general.tags": ["TODO", "FIXME", "BUG", "NOTE"],
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.tsserver.useSyntaxServer": "auto",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.paths": false,
  "typescript.tsserver.watchOptions": {
    "fallbackPolling": "dynamicPriorityPolling",
    "watchDirectory": "useFsEvents",
    "watchFile": "useFsEvents"
  },
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "top",
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Dracula Pro",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "onHover",
  "window.zoomPerWindow": false,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.fontSize": "12px",
  "errorLens.fontFamily": "Berkeley Mono",
  "scm.diffDecorations": "gutter",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "[json]": {
    "editor.wordWrap": "on"
  },
  "[html]": {
    "emmet.showExpandedAbbreviation": "always"
  },
  "vim.handleKeys": {
    "<C-c>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "C"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.mark.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.mark.next"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["[", "c"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["]", "c"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "N"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  "settingsSync.ignoredSettings": [
    "terminal.integrated.defaultProfile.osx",
    "editor.fontFamily",
    "editor.fontSize"
  ],
  "task.quickOpen.showAll": false
}
