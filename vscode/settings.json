{
  "explorer.compactFolders": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "breadcrumbs.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": true,
  "editor.fontFamily": "\"Berkeley Mono\",\"DankMono Nerd Font\", \"Berkeley Mono\", Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.suggest.showKeywords": false,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.lightbulb.enabled": "off",
  "editor.folding": true,
  "editor.showFoldingControls": "never",
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 0,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100, 120],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollBeyondLastLine": true,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.insertMode": "replace",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "emmet.showAbbreviationSuggestions": false,
	"emmet.showExpandedAbbreviation": "never",
  "errorLens.enabled": true,
  "explorer.confirmDelete": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "githubPullRequests.pullBranch": "never",
  "javascript.suggest.paths": false,
  "material-icon-theme.folders.color": "#047fb4",
  "prettier.requireConfig": true,
  "search.exclude": {
    "**/.next": true,
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true
  },
  "search.useGlobalIgnoreFiles": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "DankMono Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 1000000,
  "todo-tree.general.tags": ["TODO", "FIXME", "BUG", "NOTE"],
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.paths": false,
  "typescript.tsserver.watchOptions": {
    "fallbackPolling": "dynamicPriorityPolling",
    "watchDirectory": "useFsEvents",
    "watchFile": "useFsEvents"
  },
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "c"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "C"
      ],
      "commands": [
        "workbench.action.closeAllEditors"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.mark.prev"
      ]
    },
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.mark.next"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": ["[", "c"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["]", "c"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
  ],
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "top",
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Dracula Pro",
  "workbench.preferredLightColorTheme": "Night Owl Light",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "onHover",
  "window.zoomPerWindow": false,
  "window.zoomLevel": 0.3
}
